{
  "name": "action-docker-hub-fetch-tags",
  "version": "1.0.13",
  "private": true,
  "description": "Get Tags of a Docker image hosted on DockerHub",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map",
    "test": "jest",
    "all_wo_test": "npm run format && npm run lint && npm run build && npm run package",
    "all": "npm run all_wo_test && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MXPicture/action-docker-hub-fetch-tags.git"
  },
  "keywords": [
    "actions"
  ],
  "author": "Tobias Bildner",
  "license": "BSD",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "@types/node-fetch": "^2.6.7",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.8.7",
    "@typescript-eslint/parser": "^6.8.0",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.52.0",
    "eslint-plugin-github": "^4.10.1",
    "eslint-plugin-jest": "^27.4.3",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  }
}
